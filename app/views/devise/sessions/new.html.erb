<% provide(:title, "Sign In") %>

<div class="container">
  <div class="row margin-bottom-large">
    <div class="col-md-6 col-md-offset-3 col-sm-12 col-xs-12">
      <div class="margin-bottom-md">
        <h1 class="inline margin-right-sm js-login-switch" data-action="sign-in" >Log in</h1>
        <h1 class="inline js-login-switch" data-action="sign-up" >Sign up</h1>
      </div>
      <div class="js-user-session-form">
        <%= render 'users/sign_in' %>
      </div>
    </div>
  </div>
</div>


